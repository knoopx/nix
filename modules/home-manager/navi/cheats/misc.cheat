% misc

# random token
tr -dc A-Za-z0-9_ < /dev/urandom | head -c 32 | xargs

# downloads file list with aria2c
cat <list> | aria2c --continue=true --auto-file-renaming=false -i -c
$ list: find .

# search nixpkgs
xdg-open "https://search.nixos.org/packages?channel=unstable&type=packages&query=<text>"
$ text: gum write --placeholder "prompt..."

# search github
xdg-open "https://github.com/search?type=repositories&q=<text>"
$ text: gum write --placeholder "prompt..."
